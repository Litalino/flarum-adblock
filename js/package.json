{
  "name": "@litalino/flarum-adblock",
  "version": "1.0.0",
  "private": true,
  "prettier": "@flarum/prettier-config",
  "dependencies": {
    "flarum": "^0.1.0-beta.16",
    "flarum-webpack-config": "^2.0.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "devDependencies": {
    "@flarum/prettier-config": "^1.0.0",
    "prettier": "^3.0.3",
    "flarum-webpack-config": "^2.0.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "scripts": {
    "dev": "webpack --mode development --watch",
    "build": "webpack --mode production",
    "format": "prettier --single-quote --trailing-comma es5 --print-width 150 --tab-width 4 --write src",
    "format-check": "prettier --single-quote --trailing-comma es5 --print-width 150 --tab-width 4 --check src",
    "analyze": "cross-env ANALYZER=true yarn run build"
  }
}
